 RowBox[{
  RowBox[{"actionONSymbolic", "=", 
   RowBox[{"{", 
    RowBox[{"{", 
     RowBox[{"phi", ",", "10", ",", "even"}], "}"}], "}"}]}], ";"}]], "Input",\

 CellChangeTimes->{{3.506838218111721*^9, 3.5068382202584143`*^9}, 
   3.506838976406843*^9},
