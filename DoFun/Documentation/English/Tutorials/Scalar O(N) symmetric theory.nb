     RowBox[{"phi", "[", 
      RowBox[{"p2_", ",", " ", "b_"}], "]"}], ",", " ", 
     RowBox[{"explicit", " ", "->", " ", "True"}]}], "]"}], " ", ":=", " ", 
   RowBox[{
    RowBox[{"(", 
     RowBox[{"delta", "[", 
      RowBox[{"type", ",", " ", "a", ",", " ", "b"}], "]"}], ")"}], "/", 
    RowBox[{"(", 
