Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"fourPointAlg", "=", 
  RowBox[{
   RowBox[{
    RowBox[{"Plus", "@@", 
     RowBox[{"getAE", "[", 
      RowBox[{"fourPointTrunc", ",", 
       RowBox[{"{", 
        RowBox[{
