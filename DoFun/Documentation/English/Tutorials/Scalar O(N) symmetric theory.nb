 CellID->1290822309,ExpressionUUID->"eba1d147-f336-4aa7-aa58-5f7b8cf66118"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"zeroPointAlg", "=", 
  RowBox[{
   RowBox[{"Plus", "@@", 
    RowBox[{"getAE", "[", 
     RowBox[{"zeroPoint", ",", 
      RowBox[{"{", "}"}]}], "]"}]}], "//", "integrateDeltas"}]}]], "Input",
 CellLabel->"In[80]:=",
 CellID->1332519986,ExpressionUUID->"2f656b53-8c4e-46e3-8458-fcaeaab65581"],

Cell[BoxData[
 FractionBox[
  RowBox[{
   RowBox[{"dim", "[", "type", "]"}], " ", 
   RowBox[{"dR", "[", 
    RowBox[{
     SuperscriptBox["q1", "2"], ",", "k"}], "]"}]}], 
  RowBox[{"2", " ", 
   RowBox[{"(", 
    RowBox[{
     RowBox[{"R", "[", 
      RowBox[{
       SuperscriptBox["q1", "2"], ",", "k"}], "]"}], "+", 
     RowBox[{"Z", "[", 
      SuperscriptBox["q1", "2"], "]"}]}], ")"}]}]]], "Output",
