     RowBox[{"{", 
      RowBox[{"A", ",", "\[CurlyPhi]b"}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{"A", ",", "\[CurlyPhi]b", ",", "\[CurlyPhi]"}], "}"}]}], 
    "}"}]}], ";"}]], "Input",
