      RowBox[{
       RowBox[{"integrateDeltas", "[", 
        RowBox[{
         RowBox[{"qbqAlg", "[", 
          RowBox[{"[", "2", "]"}], "]"}], " ", "projB"}], "]"}], "//.", 
