Cell["We define some rules to deal with Dirac and Lorentz indices:", "Text",
 CellID->2037322274,
 $CellContext`ExpressionUUID -> "3d40a1cc-2f33-4cac-be62-7699ab62f550"],

Cell[BoxData[
 RowBox[{
  RowBox[{"diracRules", "=", 
   RowBox[{"{", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{
      RowBox[{"vecSlash", "[", 
       RowBox[{"p_", ",", "i_", ",", "j_"}], "]"}], "\[RuleDelayed]", 
      RowBox[{"Module", "[", 
       RowBox[{
        RowBox[{"{", "mu", "}"}], ",", 
        RowBox[{
         RowBox[{"vec", "[", 
          RowBox[{"p", ",", "mu"}], "]"}], 
         RowBox[{"gamma", "[", 
          RowBox[{"mu", ",", "i", ",", "j"}], "]"}]}]}], "]"}]}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{
       RowBox[{"gamma", "[", 
        RowBox[{"mu_", ",", "i_", ",", "j_"}], "]"}], 
       RowBox[{"gamma", "[", 
        RowBox[{"nu_", ",", "j_", ",", "i_"}], "]"}]}], "\[RuleDelayed]", 
      RowBox[{
       RowBox[{"delta", "[", 
        RowBox[{"lor", ",", "mu", ",", "nu"}], "]"}], 
       RowBox[{"dim", "[", "dirac", "]"}]}]}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{
       RowBox[{"gamma", "[", 
        RowBox[{"mu_", ",", "i_", ",", "j_"}], "]"}], 
       RowBox[{"gamma", "[", 
        RowBox[{"nu_", ",", "j_", ",", "k_"}], "]"}], 
       RowBox[{"gamma", "[", 
        RowBox[{"rho_", ",", "k_", ",", "i_"}], "]"}]}], "\[RuleDelayed]", 
      "0"}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{
       RowBox[{"gamma", "[", 
        RowBox[{"mu_", ",", "i_", ",", "j_"}], "]"}], 
       RowBox[{"gamma", "[", 
        RowBox[{"nu_", ",", "j_", ",", "k_"}], "]"}], 
       RowBox[{"gamma", "[", 
        RowBox[{"rho_", ",", "k_", ",", "l_"}], "]"}], 
       RowBox[{"gamma", "[", 
        RowBox[{"sigma_", ",", "l_", ",", "i_"}], "]"}]}], "\[RuleDelayed]", 
      RowBox[{
       RowBox[{"dim", "[", "dirac", "]"}], 
       RowBox[{"(", 
        RowBox[{
         RowBox[{
          RowBox[{"delta", "[", 
           RowBox[{"lor", ",", "mu", ",", "nu"}], "]"}], 
          RowBox[{"delta", "[", 
           RowBox[{"lor", ",", "rho", ",", "sigma"}], "]"}]}], "-", 
         RowBox[{
          RowBox[{"delta", "[", 
           RowBox[{"lor", ",", "mu", ",", "rho"}], "]"}], 
          RowBox[{"delta", "[", 
           RowBox[{"lor", ",", "nu", ",", "sigma"}], "]"}]}], "+", 
         RowBox[{
          RowBox[{"delta", "[", 
           RowBox[{"lor", ",", "mu", ",", "sigma"}], "]"}], 
          RowBox[{"delta", "[", 
           RowBox[{"lor", ",", "nu", ",", "rho"}], "]"}]}]}], ")"}]}]}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"gamma", "[", 
       RowBox[{"_", ",", "i_", ",", "i_"}], "]"}], "\[RuleDelayed]", "0"}], 
     ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"vec", "[", 
       RowBox[{
        RowBox[{"p_", "+", "q_"}], ",", "mu"}], "]"}], "\[RuleDelayed]", 
      RowBox[{
       RowBox[{"vec", "[", 
        RowBox[{"p", ",", "mu"}], "]"}], "+", 
       RowBox[{"vec", "[", 
        RowBox[{"q", ",", "mu"}], "]"}]}]}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"vec", "[", 
       RowBox[{
        RowBox[{"-", "p_"}], ",", "mu"}], "]"}], "\[RuleDelayed]", 
      RowBox[{"-", 
       RowBox[{"vec", "[", 
        RowBox[{"p", ",", "mu"}], "]"}]}]}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{
       RowBox[{"vec", "[", 
        RowBox[{"p_", ",", "mu_"}], "]"}], 
       RowBox[{"vec", "[", 
        RowBox[{"q_", ",", "mu_"}], "]"}]}], "\[RuleDelayed]", 
      RowBox[{"sp", "[", 
       RowBox[{"p", ",", "q"}], "]"}]}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{
       RowBox[{"vec", "[", 
        RowBox[{"p_", ",", "mu_"}], "]"}], "^", "2"}], "\[RuleDelayed]", 
      RowBox[{"sp", "[", "p", "]"}]}]}], "}"}]}], ";"}]], "Input",
