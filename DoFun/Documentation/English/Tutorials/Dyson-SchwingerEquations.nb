            RowBox[{"p", "-", "q1"}], ",", "\[Mu]$4243"}], "]"}], " ", 
          RowBox[{"vec", "[", 
           RowBox[{
            RowBox[{"p", "-", "q1"}], ",", "\[Nu]$4243"}], "]"}]}], 
         RowBox[{"sp", "[", 
          RowBox[{"p", "-", "q1"}], "]"}]]}], ")"}], " ", 
      RowBox[{"(", 
       RowBox[{
        RowBox[{
         RowBox[{"delta", "[", 
          RowBox[{"dirac", ",", "a$4237", ",", "b$4237"}], "]"}], " ", 
         RowBox[{"dressB", "[", 
          SuperscriptBox["q1", "2"], "]"}]}], "-", 
        RowBox[{"\[ImaginaryI]", " ", 
         RowBox[{"dressA", "[", 
          SuperscriptBox["q1", "2"], "]"}], " ", 
         RowBox[{"vecSlash", "[", 
          RowBox[{"q1", ",", "a$4237", ",", "b$4237"}], "]"}]}]}], ")"}]}], 
     ")"}], "/", 
    RowBox[{"(", 
     RowBox[{
      RowBox[{
       SuperscriptBox["q1", "2"], " ", 
       RowBox[{"dressA", "[", 
        SuperscriptBox["q1", "2"], "]"}]}], "+", 
      RowBox[{"dressB", "[", 
       SuperscriptBox["q1", "2"], "]"}]}], ")"}]}]}], "}"}]], "Output",
