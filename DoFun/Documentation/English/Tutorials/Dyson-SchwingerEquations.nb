Cell["\<\
The left-hand side of the equation is the inverse quark propagator:\
\>", "Text",
 CellID->555413753,ExpressionUUID->"a3acbfb2-0141-4898-adfe-d90c3d602fa2"],

Cell[BoxData[
 RowBox[{
  RowBox[{"lhs", "=", 
   RowBox[{
    RowBox[{"delta", "[", 
     RowBox[{"fund", ",", "a", ",", "b"}], "]"}], 
    RowBox[{"(", 
     RowBox[{
      RowBox[{"\[ImaginaryI]", " ", 
       RowBox[{"vecSlash", "[", 
        RowBox[{"p", ",", "i", ",", "j"}], "]"}], 
       RowBox[{"dressA", "[", 
        RowBox[{"p", "^", "2"}], "]"}]}], "+", 
      RowBox[{
       RowBox[{"delta", "[", 
        RowBox[{"dirac", ",", "i", ",", "j"}], "]"}], 
       RowBox[{"dressB", "[", 
        RowBox[{"p", "^", "2"}], "]"}]}]}], ")"}]}]}], ";"}]], "Input",
 CellLabel->"In[57]:=",
 CellID->1168034431,ExpressionUUID->"77a51329-1659-43e1-9d81-8145869b30bf"],

Cell["\<\
For projecting out the equations for the two dressings, we define two \
projectors:\
\>", "Text",
 CellID->1690102901,ExpressionUUID->"3e966442-6cb9-4c56-a4c1-7d1cc8f94310"],

Cell[BoxData[{
 RowBox[{
  RowBox[{"projA", "=", 
   RowBox[{
    RowBox[{
     RowBox[{
      RowBox[{"-", "1"}], "/", 
      RowBox[{"dim", "[", "dirac", "]"}]}], "/", 
     RowBox[{"dim", "[", "fund", "]"}]}], 
    RowBox[{
     RowBox[{"delta", "[", 
      RowBox[{"fund", ",", "a", ",", "b"}], "]"}], "/", 
     RowBox[{"sp", "[", "p", "]"}]}], "\[ImaginaryI]", " ", 
    RowBox[{"vecSlash", "[", 
     RowBox[{"p", ",", "j", ",", "i"}], "]"}]}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"projB", "=", 
   RowBox[{
    RowBox[{
     RowBox[{"1", "/", 
      RowBox[{"dim", "[", "dirac", "]"}]}], "/", 
     RowBox[{"dim", "[", "fund", "]"}]}], 
    RowBox[{"delta", "[", 
     RowBox[{"fund", ",", "a", ",", "b"}], "]"}], 
    RowBox[{"delta", "[", 
     RowBox[{"dirac", ",", "j", ",", "i"}], "]"}]}]}], ";"}]}], "Input",
 CellLabel->"In[58]:=",
 CellID->2130758022,ExpressionUUID->"2760d85e-fb61-4143-b476-5837a11a3c94"],

Cell["We define some rules to deal with Dirac and Lorentz indices:", "Text",
 CellID->2037322274,ExpressionUUID->"3d40a1cc-2f33-4cac-be62-7699ab62f550"],

Cell[BoxData[
 RowBox[{
  RowBox[{"diracRules", "=", 
   RowBox[{"{", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{
      RowBox[{"vecSlash", "[", 
       RowBox[{"p_", ",", "i_", ",", "j_"}], "]"}], "\[RuleDelayed]", 
      RowBox[{"Module", "[", 
       RowBox[{
        RowBox[{"{", "mu", "}"}], ",", 
        RowBox[{
         RowBox[{"vec", "[", 
          RowBox[{"p", ",", "mu"}], "]"}], 
         RowBox[{"gamma", "[", 
          RowBox[{"mu", ",", "i", ",", "j"}], "]"}]}]}], "]"}]}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{
       RowBox[{"gamma", "[", 
        RowBox[{"mu_", ",", "i_", ",", "j_"}], "]"}], 
       RowBox[{"gamma", "[", 
        RowBox[{"nu_", ",", "j_", ",", "i_"}], "]"}]}], "\[RuleDelayed]", 
      RowBox[{
       RowBox[{"delta", "[", 
        RowBox[{"lor", ",", "mu", ",", "nu"}], "]"}], 
       RowBox[{"dim", "[", "dirac", "]"}]}]}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{
       RowBox[{"gamma", "[", 
        RowBox[{"mu_", ",", "i_", ",", "j_"}], "]"}], 
       RowBox[{"gamma", "[", 
        RowBox[{"nu_", ",", "j_", ",", "k_"}], "]"}], 
       RowBox[{"gamma", "[", 
        RowBox[{"rho_", ",", "k_", ",", "i_"}], "]"}]}], "\[RuleDelayed]", 
      "0"}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{
       RowBox[{"gamma", "[", 
        RowBox[{"mu_", ",", "i_", ",", "j_"}], "]"}], 
       RowBox[{"gamma", "[", 
        RowBox[{"nu_", ",", "j_", ",", "k_"}], "]"}], 
       RowBox[{"gamma", "[", 
        RowBox[{"rho_", ",", "k_", ",", "l_"}], "]"}], 
       RowBox[{"gamma", "[", 
        RowBox[{"sigma_", ",", "l_", ",", "i_"}], "]"}]}], "\[RuleDelayed]", 
      RowBox[{
       RowBox[{"dim", "[", "dirac", "]"}], 
       RowBox[{"(", 
        RowBox[{
         RowBox[{
          RowBox[{"delta", "[", 
           RowBox[{"lor", ",", "mu", ",", "nu"}], "]"}], 
          RowBox[{"delta", "[", 
           RowBox[{"lor", ",", "rho", ",", "sigma"}], "]"}]}], "-", 
         RowBox[{
          RowBox[{"delta", "[", 
           RowBox[{"lor", ",", "mu", ",", "rho"}], "]"}], 
          RowBox[{"delta", "[", 
           RowBox[{"lor", ",", "nu", ",", "sigma"}], "]"}]}], "+", 
         RowBox[{
          RowBox[{"delta", "[", 
           RowBox[{"lor", ",", "mu", ",", "sigma"}], "]"}], 
          RowBox[{"delta", "[", 
           RowBox[{"lor", ",", "nu", ",", "rho"}], "]"}]}]}], ")"}]}]}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"gamma", "[", 
       RowBox[{"_", ",", "i_", ",", "i_"}], "]"}], "\[RuleDelayed]", "0"}], 
     ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"vec", "[", 
       RowBox[{
        RowBox[{"p_", "+", "q_"}], ",", "mu"}], "]"}], "\[RuleDelayed]", 
      RowBox[{
       RowBox[{"vec", "[", 
        RowBox[{"p", ",", "mu"}], "]"}], "+", 
       RowBox[{"vec", "[", 
        RowBox[{"q", ",", "mu"}], "]"}]}]}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"vec", "[", 
       RowBox[{
        RowBox[{"-", "p_"}], ",", "mu"}], "]"}], "\[RuleDelayed]", 
      RowBox[{"-", 
       RowBox[{"vec", "[", 
        RowBox[{"p", ",", "mu"}], "]"}]}]}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{
       RowBox[{"vec", "[", 
        RowBox[{"p_", ",", "mu_"}], "]"}], 
       RowBox[{"vec", "[", 
        RowBox[{"q_", ",", "mu_"}], "]"}]}], "\[RuleDelayed]", 
      RowBox[{"sp", "[", 
       RowBox[{"p", ",", "q"}], "]"}]}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{
       RowBox[{"vec", "[", 
        RowBox[{"p_", ",", "mu_"}], "]"}], "^", "2"}], "\[RuleDelayed]", 
      RowBox[{"sp", "[", "p", "]"}]}]}], "}"}]}], ";"}]], "Input",
 CellLabel->"In[69]:=",
 CellID->2032259154,ExpressionUUID->"9483a255-5216-4251-9335-a8518408b40e"],

Cell["And for color indices:", "Text",
