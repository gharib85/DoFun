Cell["\<\
The second term, the self-energy leads to the following expressions for the \
integrands:\
\>", "Text",
 CellID->1910471858,ExpressionUUID->"ca4b30fe-547f-4c4d-927f-f5a56a5f475e"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"integrandA", "=", 
  RowBox[{
   RowBox[{
    RowBox[{"integrateDeltas", "[", 
     RowBox[{
      RowBox[{
       RowBox[{"integrateDeltas", "[", 
        RowBox[{
         RowBox[{"qbqAlg", "[", 
          RowBox[{"[", "2", "]"}], "]"}], " ", "projA"}], "]"}], "//.", 
       "diracRules"}], "/.", "colorRules"}], "]"}], "//.", "diracRules"}], "//",
    "Simplify"}]}]], "Input",
 CellLabel->"In[74]:=",
 CellID->94459536,ExpressionUUID->"b7bd6a39-6928-4a38-8921-cfe380747e46"],

Cell[BoxData[
 RowBox[{
  RowBox[{"(", 
   RowBox[{"CF", " ", 
    SuperscriptBox["g", "2"], " ", 
    RowBox[{"dressA", "[", 
     SuperscriptBox["q1", "2"], "]"}], " ", 
    RowBox[{"dressGamma", "[", 
     RowBox[{
      RowBox[{"p", "-", "q1"}], ",", 
      RowBox[{"-", "p"}], ",", "q1"}], "]"}], " ", 
    RowBox[{"dressZ", "[", 
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{"p", "-", "q1"}], ")"}], "2"], "]"}], " ", 
    RowBox[{"(", 
     RowBox[{
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         RowBox[{"-", "3"}], "+", 
         RowBox[{"dim", "[", "lor", "]"}]}], ")"}], " ", 
       RowBox[{"sp", "[", 
        RowBox[{"p", "-", "q1"}], "]"}], " ", 
       RowBox[{"sp", "[", 
        RowBox[{"p", ",", "q1"}], "]"}]}], "+", 
      RowBox[{"2", " ", 
       RowBox[{"sp", "[", 
        RowBox[{"p", ",", 
         RowBox[{"p", "-", "q1"}]}], "]"}], " ", 
       RowBox[{"sp", "[", 
        RowBox[{
         RowBox[{"p", "-", "q1"}], ",", "q1"}], "]"}]}]}], ")"}]}], ")"}], 
  "/", 
  RowBox[{"(", 
   RowBox[{
    RowBox[{"(", 
     RowBox[{
      RowBox[{
       SuperscriptBox["q1", "2"], " ", 
       RowBox[{"dressA", "[", 
        SuperscriptBox["q1", "2"], "]"}]}], "+", 
      RowBox[{"dressB", "[", 
       SuperscriptBox["q1", "2"], "]"}]}], ")"}], " ", 
    RowBox[{"sp", "[", "p", "]"}], " ", 
    RowBox[{"sp", "[", 
     RowBox[{"p", "-", "q1"}], "]"}]}], ")"}]}]], "Output",
 ImageSize->{540, 78},
 ImageMargins->{{0, 0}, {0, 0}},
 ImageRegion->{{0, 1}, {0, 1}},
 CellLabel->"Out[74]=",
 CellID->844298445,ExpressionUUID->"90c9b56f-997a-4a1d-8ae1-47c7c3b3482a"]
}, Open  ]],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"integrandB", "=", 
  RowBox[{
   RowBox[{
    RowBox[{"integrateDeltas", "[", 
