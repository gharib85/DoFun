Cell[BoxData[
 RowBox[{"colorRules", ":=", 
  RowBox[{"{", 
   RowBox[{
    RowBox[{
     RowBox[{"T", "[", 
      RowBox[{"a_", ",", "i_", ",", "j_"}], "]"}], 
