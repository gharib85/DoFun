Cell[BoxData[{
 RowBox[{"Clear", "@", "setFields"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"setFields", "::", "usage"}], "=", 
    "\"\<Sets the properties of the fields of an action.\\n\nSyntax:\n\
setFields[bosons, grassmann, complex] where the arguments are lists \
containing bosons, pairs of Grassmannian fields and pairs of bosonic complex \
fields.\nBosons are always single entries, while Grassmannian fields and \
bosonic complex fields come as pairs in lists. The components of pairs are \
defined as mutual anti-fields.\\n\nExample: Definition of a bosonic field A, \
a pair of anti-commuting fields c and cb and a pair of bosonic complex fields \
phi and phib.\nsetFields[{A}, {{c, cb}}, {{phi, phib}}];\nbosonQ /@ {A, c, \
cb, phi, phib}\nfermionQ /@ {A, c, cb, phi, phib}\nantiComplexFieldQ /@ {A, \
c, cb, phi, phib}\nantiField/@{A,c,cb,phi,phib}\n\>\""}], ";"}], 
  "\[IndentingNewLine]", 
  RowBox[{"(*", " ", 
   RowBox[{"some", " ", "defaults"}], " ", "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"setFields", "[", "bosons_List", "]"}], ":=", 
  RowBox[{"setFields", "[", 
   RowBox[{"bosons", ",", 
    RowBox[{"{", "}"}], ",", 
    RowBox[{"{", "}"}]}], "]"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"setFields", "[", 
    RowBox[{"bosons_List", ",", "fermions_List"}], "]"}], ":=", 
   RowBox[{"setFields", "[", 
    RowBox[{"bosons", ",", "fermions", ",", 
     RowBox[{"{", "}"}]}], "]"}]}], "\[IndentingNewLine]", 
  RowBox[{"(*", " ", 
   RowBox[{
   "check", " ", "syntax", " ", "of", " ", "fermionic", " ", "and", " ", 
    "complex", " ", "fields", " ", 
    RowBox[{"(", 
     RowBox[{"need", " ", "to", " ", "be", " ", "pairs"}], ")"}]}], " ", 
   "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"setFields", "[", 
     RowBox[{"bosons_List", ",", "fermions_List", ",", "complexFields_List"}],
      "]"}], "/;", 
    RowBox[{"Not", "[", 
     RowBox[{"And", "@@", 
      RowBox[{"Flatten", "[", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{
          RowBox[{
           RowBox[{
            RowBox[{"Head", "@", "#"}], "===", "List"}], "&"}], "/@", 
          "fermions"}], ",", 
         RowBox[{
          RowBox[{
           RowBox[{
            RowBox[{"Head", "@", "#"}], "===", "List"}], "&"}], "/@", 
          "complexFields"}]}], "}"}], "]"}]}], "]"}]}], ":=", 
   RowBox[{"Message", "[", 
    RowBox[{"setFields", "::", "noList"}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"setFields", "[", 
    RowBox[{"bosons_List", ",", "fermions_List", ",", "complexFields_List"}], 
    "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", "}"}], ",", "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"(*", " ", 
      RowBox[{"set", " ", "the", " ", "field", " ", "types"}], " ", "*)"}], 
     "\[IndentingNewLine]", 
     RowBox[{"(*", " ", 
      RowBox[{"difference", " ", "to", " ", 
       RowBox[{"DoFun2", ":", " ", 
        RowBox[{
        "complex", " ", "fields", " ", "are", " ", "their", " ", "own", " ", 
         "types"}]}]}], " ", "*)"}], "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{
       RowBox[{
        RowBox[{"(", 
         RowBox[{"#", "/:", 
          RowBox[{"fieldType", "[", "#", "]"}], ":=", "boson"}], ")"}], "&"}],
        "/@", "bosons"}], ";", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{
        RowBox[{"(", 
         RowBox[{"#", "/:", 
          RowBox[{"fieldType", "[", "#", "]"}], ":=", "fermion"}], ")"}], 
        "&"}], "/@", 
       RowBox[{"fermions", "[", 
        RowBox[{"[", 
         RowBox[{"All", ",", "1"}], "]"}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{
        RowBox[{"(", 
         RowBox[{"#", "/:", 
          RowBox[{"fieldType", "[", "#", "]"}], ":=", "antiFermion"}], ")"}], 
        "&"}], "/@", 
       RowBox[{"fermions", "[", 
        RowBox[{"[", 
         RowBox[{"All", ",", "2"}], "]"}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{
        RowBox[{"(", 
         RowBox[{"#", "/:", 
          RowBox[{"fieldType", "[", "#", "]"}], ":=", "complex"}], ")"}], 
        "&"}], "/@", 
       RowBox[{"complexFields", "[", 
        RowBox[{"[", 
         RowBox[{"All", ",", "1"}], "]"}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{
        RowBox[{"(", 
         RowBox[{"#", "/:", 
          RowBox[{"fieldType", "[", "#", "]"}], ":=", "antiComplex"}], ")"}], 
        "&"}], "/@", 
       RowBox[{"complexFields", "[", 
        RowBox[{"[", 
         RowBox[{"All", ",", "2"}], "]"}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"(*", " ", 
       RowBox[{"internally", " ", "used", " ", "dummy", " ", "fields"}], " ", 
       "*)"}], "\[IndentingNewLine]", 
      RowBox[{"$dummyFieldF", "/:", 
       RowBox[{"fieldType", "[", "$dummyFieldF", "]"}], ":=", "fermion"}], 
      ";", "\[IndentingNewLine]", 
      RowBox[{"$dummyFieldAF", "/:", 
       RowBox[{"fieldType", "[", "$dummyFieldAF", "]"}], ":=", 
       "antiFermion"}], ";", "\[IndentingNewLine]", "\[IndentingNewLine]", 
      RowBox[{"(*", " ", 
       RowBox[{
       "set", " ", "all", " ", "fields", " ", "to", " ", "Head", " ", 
        "field"}], " ", "*)"}], "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{
        RowBox[{"(", 
         RowBox[{"#", "/:", 
          RowBox[{"Head", "[", "#", "]"}], "=", "field"}], ")"}], "&"}], "/@", 
       RowBox[{"Flatten", "[", 
        RowBox[{"{", 
         RowBox[{"bosons", ",", "fermions", ",", "complexfields"}], "}"}], 
        "]"}]}], ";", "\[IndentingNewLine]", "\[IndentingNewLine]", 
      RowBox[{"(*", " ", 
       RowBox[{"set", " ", 
        RowBox[{"(", 
         RowBox[{"anti", "-"}], ")"}], "commutating", " ", "property"}], " ", 
       "*)"}], "\[IndentingNewLine]", 
      RowBox[{"(*", " ", 
       RowBox[{"TODO", ":", " ", 
        RowBox[{"put", " ", "this", " ", "in", " ", "the", " ", "init"}]}], 
       " ", "*)"}], "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{
        RowBox[{"(", 
         RowBox[{"#", "/:", 
          RowBox[{"grassmannQ", "[", "#", "]"}], "=", "False"}], ")"}], "&"}],
        "/@", 
       RowBox[{"Flatten", "[", 
        RowBox[{"{", 
         RowBox[{"bosons", ",", "complexFields"}], "}"}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{
        RowBox[{"(", 
         RowBox[{"#", "/:", 
          RowBox[{"grassmannQ", "[", "#", "]"}], "=", "True"}], ")"}], "&"}], 
       "/@", 
       RowBox[{"Flatten", "[", 
        RowBox[{"{", 
         RowBox[{"fermions", ",", "$dummyFieldF", ",", "$dummyFieldAF"}], 
         "}"}], "]"}]}], ";", "\[IndentingNewLine]", "\[IndentingNewLine]", 
      RowBox[{"(*", " ", 
       RowBox[{
        RowBox[{"define", " ", "the", " ", "anti"}], "-", "fields"}], " ", 
       "*)"}], "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{
        RowBox[{"(", 
         RowBox[{
          RowBox[{"antiField", "[", 
           RowBox[{"#", "[", 
            RowBox[{"[", "1", "]"}], "]"}], "]"}], "=", 
          RowBox[{"#", "[", 
           RowBox[{"[", "2", "]"}], "]"}]}], ")"}], "&"}], "/@", 
       RowBox[{"Transpose", "[", 
        RowBox[{"{", 
         RowBox[{"bosons", ",", "bosons"}], "}"}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{
        RowBox[{"(", 
         RowBox[{
          RowBox[{"antiField", "[", 
           RowBox[{"#", "[", 
            RowBox[{"[", "1", "]"}], "]"}], "]"}], "=", 
          RowBox[{"#", "[", 
           RowBox[{"[", "2", "]"}], "]"}]}], ")"}], "&"}], "/@", "fermions"}],
       ";", 
      RowBox[{
       RowBox[{
        RowBox[{"(", 
         RowBox[{
          RowBox[{"antiField", "[", 
           RowBox[{"#", "[", 
            RowBox[{"[", "2", "]"}], "]"}], "]"}], "=", 
          RowBox[{"#", "[", 
           RowBox[{"[", "1", "]"}], "]"}]}], ")"}], "&"}], "/@", "fermions"}],
       ";", 
      RowBox[{
       RowBox[{
        RowBox[{"(", 
         RowBox[{
          RowBox[{"antiField", "[", 
           RowBox[{"#", "[", 
            RowBox[{"[", "1", "]"}], "]"}], "]"}], "=", 
